{
  "expo": {
    "name": "Ayurlekha",
    "slug": "ayurlekha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Ayurlekha-logo.png",
    "scheme": "ayurlekha",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Ayurlekha-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aefaeb68-25ec-426d-b08e-d89c5ef6188c"
      }
    },
    "android": {
      "package": "com.optuze.ayurlekha"
    }
  }
}
